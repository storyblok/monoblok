{
  "name": "@storyblok/management-api-client",
  "version": "0.1.9",
  "type": "module",
  "description": "Storyblok Management API Client",
  "main": "./dist/index.cjs",
  "private": false,
  "module": "./dist/index.js",
  "types": "./dist/index.d.cts",
  "homepage": "https://github.com/storyblok/monoblok/tree/main/packages/mapi-client#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/storyblok/monoblok.git",
    "directory": "packages/mapi-client"
  },
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/storyblok/monoblok/issues"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json",
    "./resources/*": {
      "import": {
        "types": "./dist/generated/*/types.gen.d.ts",
        "default": "./dist/generated/*/types.gen.js"
      },
      "require": {
        "types": "./dist/generated/*/types.gen.d.cts",
        "default": "./dist/generated/*/types.gen.cjs"
      }
    }
  },
  "scripts": {
    "generate": "tsx generate.ts",
    "build": "tsdown",
    "test": "vitest"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@storyblok/region-helper": "workspace:*"
  },
  "devDependencies": {
    "@hey-api/openapi-ts": "^0.80.1",
    "@storyblok/openapi": "workspace:*",
    "@types/node": "^24.1.0",
    "change-case": "^5.4.4",
    "glob": "^10.3.10",
    "mustache": "^4.2.0",
    "tsdown": "^0.20.3",
    "tsx": "^4.20.3",
    "vitest": "^3.1.3"
  },
  "nx": {
    "targets": {
      "generate": {
        "dependsOn": [
          "^build"
        ],
        "outputs": [
          "{projectRoot}/src/generated/**",
          "{projectRoot}/src/sdk-registry.generated.ts",
          "{projectRoot}/src/types.generated.ts"
        ]
      },
      "build": {
        "dependsOn": [
          "generate",
          "^build"
        ],
        "outputs": [
          "{projectRoot}/dist/**"
        ]
      }
    }
  }
}
