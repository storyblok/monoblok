  },
  "devDependencies": {
    "@storyblok/eslint-config": "workspace:*",
    "@vitest/coverage-v8": "^3.1.3",
    "@vitest/ui": "^3.1.3",
    "eslint": "^9.26.0",
    "msw": "^2.10.2",
    "tsdown": "latest",
    "vite": "^6.0.7",
    "vitest": "^3.1.3"
