{
  "name": "storyblok",
  "type": "module",
  "version": "4.14.1",
  "packageManager": "pnpm@10.10.0",
  "description": "Storyblok CLI",
  "author": "Alvaro Saburido <hola@alvarosaburido.dev> (https://github.com/alvarosabu/)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/storyblok/monoblok.git",
    "branch": "main",
    "directory": "packages/cli"
  },
  "keywords": [
    "storyblok",
    "cli",
    "node",
    "javascript"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    },
    "./config": {
      "types": "./dist/config/index.d.ts",
      "import": "./dist/config/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "storyblok": "./dist/index.mjs"
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "unbuild",
    "release": "release-it",
    "dev": "pnpm run build && node dist/index.mjs",
    "debug": "node --inspect-brk --loader tsx src/index.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:types": "tsc --noEmit --skipLibCheck",
    "test:ci": "vitest run",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@inquirer/prompts": "^7.5.1",
    "@storyblok/js": "workspace:*",
    "@storyblok/management-api-client": "workspace:*",
    "@storyblok/region-helper": "workspace:*",
    "@topcli/spinner": "^2.1.2",
    "async-sema": "^3.1.1",
    "c12": "^3.3.0",
    "chalk": "^5.4.1",
    "cli-progress": "^3.12.0",
    "commander": "^13.1.0",
    "dotenv": "^16.5.0",
    "filenamify": "^6.0.0",
    "json-schema-to-typescript": "^15.0.4",
    "minimatch": "^10.0.3",
    "octokit": "^5.0.3",
    "ohash": "^2.0.11",
    "open": "^10.1.2",
    "pathe": "^2.0.3",
    "read-package-up": "^11.0.0",
    "storyblok-js-client": "workspace:*"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "10.0.0",
    "@storyblok/eslint-config": "workspace:*",
    "@types/cli-progress": "^3.11.6",
    "@types/inquirer": "^9.0.8",
    "@types/node": "^22.15.18",
    "@vitest/coverage-v8": "^3.1.3",
    "@vitest/ui": "^3.1.3",
    "eslint": "^9.26.0",
    "memfs": "^4.17.1",
    "msw": "^2.8.2",
    "release-it": "^18.1.2",
    "typescript": "5.8.3",
    "unbuild": "^3.5.0",
    "uuid": "^11.1.0",
    "vite": "^6.3.5",
    "vitest": "^3.1.3"
  }
}
