{
  "name": "@storyblok/mapi-client",
  "version": "0.0.0",
  "description": "Storyblok MAPI Client",
  "main": "src/index.ts",
  "scripts": {
    "generate": "./build.sh",
    "build": "tsdown",
    "test": "vitest"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@10.10.0",
  "dependencies": {
    "@hey-api/openapi-ts": "^0.80.1",
    "@storyblok/openapi": "workspace:*",
    "@storyblok/region-helper": "workspace:*",
    "tsdown": "^0.12.9",
    "vitest": "^3.1.3"
  },
  "nx": {
    "targets": {
      "generate": {
        "dependsOn": [
          "^build"
        ],
        "outputs": [
          "{projectRoot}/src/generated/**"
        ]
      },
      "build": {
        "dependsOn": [
          "generate",
          "^build"
        ],
        "outputs": [
          "{projectRoot}/dist/**"
        ]
      }
    }
  }
}
