{
  "name": "@storyblok/migrations",
  "type": "module",
  "version": "0.0.1",
  "private": false,
  "packageManager": "pnpm@10.10.0",
  "description": "Migration utilities for Storyblok",
  "author": "Storyblok",
  "license": "MIT",
  "homepage": "https://github.com/storyblok/monoblok/tree/main/packages/migrations#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/storyblok/monoblok.git",
    "directory": "packages/migrations"
  },
  "bugs": {
    "url": "https://github.com/storyblok/monoblok/issues"
  },
  "keywords": [
    "storyblok",
    "migrations",
    "content"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "unbuild",
    "dev": "vitest dev",
    "play": "jiti playground",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepack": "pnpm run build",
    "test": "pnpm lint && pnpm test:types && vitest run --coverage",
    "test:unit": "vitest",
    "test:unit:ci": "vitest run",
    "test:types": "tsc --noEmit --skipLibCheck",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@storyblok/richtext": "workspace:*"
  },
  "devDependencies": {
    "@storyblok/eslint-config": "workspace:*",
    "@storyblok/management-api-client": "workspace:*",
    "@types/node": "^22.15.18",
    "@vitest/coverage-v8": "^3.1.3",
    "eslint": "^9.26.0",
    "jiti": "^2.4.2",
    "typescript": "5.8.3",
    "unbuild": "^3.5.0",
    "vitest": "^3.1.3"
  },
  "nx": {
    "targets": {
      "build": {
        "dependsOn": [
          "^build"
        ],
        "outputs": [
          "{projectRoot}/dist/**"
        ]
      }
    }
  }
}
