{
  "name": "@storyblok/management-client",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "description": "Storyblok Management API Client",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20"
  },
  "scripts": {
