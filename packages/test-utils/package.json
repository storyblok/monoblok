{
  "name": "@storyblok/test-utils",
  "version": "1.0.0",
  "description": "Storyblok Test Utils",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "homepage": "https://github.com/storyblok/monoblok/tree/main/packages/test-utils#readme",
  "repository": {
    "type": "git",
